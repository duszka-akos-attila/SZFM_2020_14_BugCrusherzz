<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:layout_marginBottom="2dp"
    >


    <TextView
        android:id="@+id/data_textView"
        android:layout_width="107dp"
        android:layout_height="35dp"
        android:layout_marginStart="275dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="20dp"
        android:text="-1000"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"

        />

    <TextView
        android:id="@+id/nev_textView"
        android:layout_width="178dp"
        android:layout_height="26dp"
        android:text="Sorocske"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toTopOf="@+id/datum_textView"
        app:layout_constraintEnd_toStartOf="@+id/data_textView"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toEndOf="@+id/datum_textView"
        app:layout_constraintTop_toTopOf="@+id/data_textView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/datum_textView"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginBottom="10dp"
        android:text="2020.05.20"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/data_textView"
        app:layout_constraintHorizontal_bias="0.018"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/data_textView" />

    <TextView
        android:id="@+id/osztaly_textView"
        android:layout_width="113dp"
        android:layout_height="18dp"
        android:layout_marginEnd="72dp"
        android:text="Etel-ital"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        app:layout_constraintEnd_toStartOf="@+id/data_textView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nev_textView"
        app:layout_constraintTop_toBottomOf="@+id/nev_textView" />
</androidx.constraintlayout.widget.ConstraintLayout>